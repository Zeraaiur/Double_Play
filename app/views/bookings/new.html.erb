<%= simple_form_for [@item, @booking] do |f| %>
  <div>
<div class="container">
    <div class="row">
        <div class='col-sm-6'>
            <div class="form-group">
                <div class='input-group date' id='datetimepicker1'>
              <%= f.input :pick_time, as: :string %>
